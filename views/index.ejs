<div class="author">
  <h1 class="author_heading">Hi, I am Sean.</h1>
  <p class="author_body">
    Frontend/Fullstack developer, interested in DATA, former SP and ATS
    @Microsoft.
  </p>
</div>

<img src="/img/Hero.jpg" alt="" class="hero-image" width="981" height="528" />

<section class="articles">
  <h2 class="articles__heading">Latest Posts</h2>

  <ul class="article-ul">
    <% data.forEach(post => { %>

    <li>
      <a href="#">
        <span><%= post.title %></span>
        <span class="article-list__date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>

    <% }) %>
  </ul>

  <a href="#" class="pagination">&lt; View Older Page</a>
</section>
